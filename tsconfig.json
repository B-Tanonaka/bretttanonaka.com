{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "allowJs": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "outDir": "./client/build",
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false,
    "declaration": true,
  }
}

{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "outDir": "./client/dist",
    "rootDir": "client/src",
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
